	<service name="obs_scm">
		<param name="extract">Datensicherung.spec</param>
		<param name="filename">Datensicherung</param>
		<param name="revision">Datensicherung</param>
		<param name="scm">git</param>
		<param name="submodules">disable</param>
		<param name="url">https://github.com/olafhering/scripts</param>
		<param name='versionformat'>%cd.%h</param>
	</service>
	<service mode="buildtime" name="set_version">
		<param name="basename">Datensicherung</param>
	</service>
</services>
